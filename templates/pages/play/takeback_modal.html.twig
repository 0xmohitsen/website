<div class="modal fade" id="takebackModal" data-bs-backdrop="static" tabindex="-1" aria-hidden="true">
<div class="modal-dialog">
  <div class="modal-content">
    <div class="modal-body">
      <div class="alert alert-light d-flex align-items-center" role="alert">
        {% include 'svg/info-circle-fill.svg' %}
        <div class="message">
          A takeback is being proposed.
        </div>
      </div>
      <form>
        <button class="btn btn-primary" data-bs-dismiss="modal">Accept</button>
        <button class="btn btn-secondary" data-bs-dismiss="modal">Decline</button>
      </form>
    </div>
  </div>
</div>
</div>
